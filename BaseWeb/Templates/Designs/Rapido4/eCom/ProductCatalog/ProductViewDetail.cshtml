@inherits ViewModelTemplate<ProductViewModel>
@using Dynamicweb.Rendering
@using Dynamicweb.Ecommerce.ProductCatalog
@using System.Linq


@{ 
    if (System.Web.HttpContext.Current.Items.Contains("ProductDetails"))
    {
        System.Web.HttpContext.Current.Items["ProductDetails"] = Model;
    } 
    else 
    {
        System.Web.HttpContext.Current.Items.Add("ProductDetails", Model);
    }
}